---
import { ANALYTICS } from 'astrowind:config';
---

{
  ANALYTICS?.vendors?.goatcounter?.id ? (
    <script data-goatcounter=`https://${String(ANALYTICS?.vendors?.goatcounter?.id)}.goatcounter.com/count`
        async src="//gc.zgo.at/count.js"/>
  ) : null
}
{
    ANALYTICS?.vendors?.plausible?.api ? (
    <script
            data-domain={ANALYTICS?.vendors?.plausible?.domain}
            data-api={ANALYTICS?.vendors?.plausible?.api}
            src="/js/plausible.js">
    </script>
        ) : null
    }
